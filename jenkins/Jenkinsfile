pipeline {
   agent any
  tools {nodejs "node"}
    environment {
        CI = 'true'
    }
    stages {
        stage('Build') {
            steps {
                git 'https://github.com/thongvm316/eCommerce-MERNStack-FE.git'
                sh 'npm install'
            }
        }
    
        stage('Deliver') {
            steps {
                sh 'npm run deploy'
            }
        }
    }
}

